# TenSafe Finance Demonstrator â€” Environment Configuration
# Copy this file to .env and adjust values for your deployment.
#
# All variables are prefixed with TENSAFE_ and loaded automatically
# by the DemoConfig (demonstrator/server/config.py).

# Path to the MoE config JSON (expert definitions + HE params)
TENSAFE_MOE_CONFIG_PATH=demonstrator/adapters/tgsp/moe_config.json

# PyTorch device: "cuda" for GPU, "cpu" for CPU-only
TENSAFE_DEVICE=cuda

# Comma-separated allowed CORS origins
TENSAFE_CORS_ORIGINS=http://localhost:8000,http://localhost:3000

# Max requests per minute per IP (rate limiting)
TENSAFE_RATE_LIMIT_RPM=60

# Max simultaneous GPU-bound generation tasks
TENSAFE_MAX_CONCURRENT_GENERATIONS=2

# Environment: "development" (Swagger UI enabled) or "production"
TENSAFE_TG_ENVIRONMENT=development

# Python logging level: DEBUG, INFO, WARNING, ERROR
TENSAFE_LOG_LEVEL=INFO
